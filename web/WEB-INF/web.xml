<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Servlet01</servlet-name>
        <servlet-class>ServlerPackage.Servlet01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet01</servlet-name>
        <url-pattern>/ServlerPackage/Servlet01</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>ServlerPackage.UserServletGroup.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/ServlerPackage/UserServletGroup/UserServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SiveServlet</servlet-name>
        <servlet-class>ServlerPackage.UserServletGroup.SiveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SiveServlet</servlet-name>
        <url-pattern>/ServlerPackage/UserServletGroup/SiveServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateUserServlet</servlet-name>
        <servlet-class>ServlerPackage.UserServletGroup.UpdateUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateUserServlet</servlet-name>
        <url-pattern>/ServlerPackage/UserServletGroup/UpdateUserServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>ServlerPackage.UserServletGroup.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/ServlerPackage/UserServletGroup/LoginServlet</url-pattern>
    </servlet-mapping>
</web-app>
